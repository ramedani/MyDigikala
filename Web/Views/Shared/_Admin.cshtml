    document.addEventListener("DOMContentLoaded", function () {

        // تمام لینک‌های داخل منو
        const menuLinks = document.querySelectorAll("#main-menu-navigation a");

        menuLinks.forEach(link => {
            link.addEventListener("click", function () {

                // 1. حذف active از همه‌ی li های منو
                document.querySelectorAll("#main-menu-navigation li").forEach(li => {
                    li.classList.remove("active");
                });

                // 2. اضافه کردن active به li مربوط به همان لینک
                this.closest("li").classList.add("active");

            });
        });

    });Admin
</script>

