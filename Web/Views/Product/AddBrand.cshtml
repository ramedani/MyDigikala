@model Application.DTO.EditBrandDto


@{
    Layout = "_admin";
}

<form asp-action="AddBrand" asp-controller="Product" method="post" enctype="multipart/form-data">

    <br/>
    <label asp-for="NameF"></label>
    <input asp-for="NameF" class="form-control" required />
    <span asp-validation-for="NameF" class="text-danger"></span>
    <input type="hidden" asp-for="Id" />

    <button type="submit" class="btn btn-primary">Save</button>
</form>